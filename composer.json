    "reference": "origin/master"
  },
  "license": "GPL",
  "authors": [{
    "name": "Edan Schwartz",
    "email": "edanschwartz@gmail.com"
  }, {
    "name": "Seth Miller",
    "email": "seth@four43.com"
  }],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.3.3",
    "guzzlehttp/guzzle": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "Aeris\\GuzzleHttpMock\\": "src/",
      "Aeris\\GuzzleHttpMockTest\\": "test/GuzzleHttpMockTest'"
